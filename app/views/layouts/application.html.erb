<!DOCTYPE html>
<html>
  <head>
    <title>Ellii</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>

    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload', defer: true %>
    <link rel="icon" type="image/png" href="<%= image_path("logo.png") %>"/>
    <%= stylesheet_pack_tag 'application' %>
    <script type="text/javascript">window.$crisp=[];window.CRISP_WEBSITE_ID="a248a030-0a0a-4286-a9dd-8b5306d53568";(function(){d=document;s=d.createElement("script");s.src="https://client.crisp.chat/l.js";s.async=1;d.getElementsByTagName("head")[0].appendChild(s);})();</script>
  </head>

  <body>
    <%= render 'shared/navbar' %>
    <%= render 'shared/flashes' %>
    <div class="content-wrapper" style="min-height: calc(100vh - 70px - 120px);">
      <%= yield %>
    </div>
    <%= render 'shared/footer' %>

    <script src="https://media.twiliocdn.com/sdk/js/video/v1/twilio-video.min.js"></script>
  </body>
</html>
