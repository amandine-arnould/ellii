<% if score == 1 %>
  <i id="frown" class="far fa-frown"></i>
<% elsif score == 2 %>
  <i id="meh" class="far fa-meh"></i>
<% elsif score == 3 %>
  <i id="smile" class="far fa-smile"></i>
<% elsif score == 4 %>
  <i id="smile-beam" class="far fa-smile-beam"></i>
<% elsif score == 5 %>
  <i id="laugh-beam" class="far fa-laugh-beam"></i>
<% end %>
